{
  "name": "@operational/visualizations-stories",
  "private": true,
  "version": "0.0.1",
  "description": "Stories of @operational/visualizations rendering libraries for E2E tests",
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 7000",
    "storybook": "start-storybook -p 7000",
    "test": "./node_modules/.bin/chromatic test --app-code=\"lh0tyo268i\"",
    "test:stories": "./node_modules/.bin/chromatic test --app-code=\"lh0tyo268i\" --exit-zero-on-changes",
    "test:stories-auto": "./node_modules/.bin/chromatic test --app-code=\"lh0tyo268i\" --auto-accept-changes",
    "build": "build-storybook -o dist",
    "build-storybook": "build-storybook -o dist",
    "clean": "rimraf tsconfig.tsbuildinfo lib dist"
  },
  "dependencies": {
    "@operational/frame": "^0.3.10",
    "@operational/grid": "^0.3.5",
    "@operational/visualizations": "^7.0.22",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "react-virtualized-auto-sizer": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addons": "5.2.1",
    "@storybook/core": "5.3.13",
    "@storybook/react": "5.0.11",
    "@types/storybook__addon-info": "^4.1.1",
    "@types/storybook__react": "4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "8.0.6",
    "react-docgen-typescript-loader": "^3.1.0",
    "storybook-chromatic": "2.2.2"
  }
}
